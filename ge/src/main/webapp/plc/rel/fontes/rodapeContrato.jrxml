				<textElement textAlignment="Justified" verticalAlignment="Top">
					<font fontName="Tahoma" size="8"/>
				</textElement>
				<text><![CDATA[Por acordo entre as partes;]]></text>
			</staticText>
			<staticText>
				<reportElement x="13" y="64" width="22" height="14" uuid="02b2fd35-8a15-48f5-99f2-2ebb3b39bb92"/>
