				<textElement textAlignment="Justified" verticalAlignment="Top">
					<font fontName="Tahoma" size="8"/>
				</textElement>
				<text><![CDATA[Dos CONTRATANTES: Cópia do comprovante de endereço; Cópia do CPF; Cópia do RG.]]></text>
			</staticText>
			<staticText>
				<reportElement positionType="Float" mode="Opaque" x="5" y="211" width="24" height="41" uuid="3e781ecf-2437-49fc-83ed-d541ecdc574a"/>
